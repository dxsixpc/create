{
  "name": "name",
  "version": "0.0.1",
  "description": "description",
  "homepage": "https://dxsixpc.github.io/dumi-react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxsixpc/dumi-react"
  },
  "license": "MIT",
  "author": "author",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "commit": "git add . && git-cz",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "doctor": "father doctor",
    "prepublishOnly": "father doctor && npm run build",
    "start": "npm run dev"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cz-commitlint": "^17.4.2",
    "@dxsixpc/configs": "^0.3.0",
    "@umijs/lint": "^4.0.51",
    "commitizen": "^4.0.3",
    "dumi": "^2.1.9",
    "eslint": "^8.33.0",
    "father": "^4.1.4",
    "inquirer": "^8.0.0",
    "lint-staged": "^13.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.6",
    "stylelint": "^15.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
