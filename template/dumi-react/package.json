{
  "name": "name",
  "version": "0.0.1",
  "description": "description",
  "homepage": "https://dxsixpc.github.io/dumi-react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxsixpc/dumi-react"
  },
  "license": "MIT",
  "author": "author",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "commit": "git add . && git-cz",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "doctor": "father doctor",
    "prepublishOnly": "father doctor && npm run build",
    "start": "npm run dev"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": ">=7.8.0 <8.0.0",
    "@dxsixpc/configs": "^0.3.3",
    "@umijs/lint": "^4.0.52",
    "dumi": "^2.1.10",
    "eslint": ">=7.5.0 <8.0.0 || >=8.0.0-0 <8.0.0 || >=8.0.0 <9.0.0",
    "father": "^4.1.5",
    "lint-staged": "^13.1.2",
    "postcss": ">=8.4.6 <9.0.0",
    "prettier": ">=2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "styled-components": ">= 2",
    "stylelint": "^15.2.0",
    "typescript": ">=4.7.0 <5.0.0",
    "webpack": ">=5.20.0 <6.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
