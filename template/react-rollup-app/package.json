{
  "name": "react-rollup-app",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "commit": "git add . && git-cz",
    "dev": "rollup -c -w",
    "start": "npm run dev"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.0.0-rc.0",
    "@emotion/styled": "^11.10.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": ">=7.0.0 <8.0.0",
    "@commitlint/cz-commitlint": "^17.4.2",
    "@dxsixpc/configs": "0.3.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "commitizen": "^4.0.3",
    "inquirer": "^8.0.0",
    "rollup": "^3.15.0"
  }
}
